{
  "name": "jupyter-live-share",
  "displayName": "Jupyter Live Share",
  "description": "Share Jupyter Notebooks in real-time with students via browser",
  "version": "2.0.0",
  "publisher": "jupyter-live-share",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SeolMuah/jupyter-live-share"
  },
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Education",
    "Notebooks"
  ],
  "activationEvents": [
    "onNotebook:jupyter-notebook",
    "onCommand:jupyterLiveShare.startSession",
    "onCommand:jupyterLiveShare.openViewer"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jupyterLiveShare.startSession",
        "title": "Start Session",
        "category": "Jupyter Live Share"
      },
      {
        "command": "jupyterLiveShare.stopSession",
        "title": "Stop Session",
        "category": "Jupyter Live Share"
      },
      {
        "command": "jupyterLiveShare.openViewer",
        "title": "Open Viewer",
        "category": "Jupyter Live Share"
      },
      {
        "command": "jupyterLiveShare.createPoll",
        "title": "Create Poll",
        "category": "Jupyter Live Share"
      },
      {
        "command": "jupyterLiveShare.endPoll",
        "title": "End Poll",
        "category": "Jupyter Live Share"
      }
    ],
    "configuration": {
      "title": "Jupyter Live Share",
      "properties": {
        "jupyterLiveShare.port": {
          "type": "number",
          "default": 48632,
          "description": "Local server port for the live share session"
        },
        "jupyterLiveShare.maxViewers": {
          "type": "number",
          "default": 100,
          "description": "Maximum number of concurrent viewers"
        },
        "jupyterLiveShare.tunnelProvider": {
          "type": "string",
          "enum": [
            "cloudflare",
            "none"
          ],
          "default": "cloudflare",
          "description": "Tunnel provider for external access"
        },
        "jupyterLiveShare.imageMaxWidth": {
          "type": "number",
          "default": 1280,
          "description": "Maximum image width in pixels for optimization (default: 1280)"
        },
        "jupyterLiveShare.imageMaxSizeKB": {
          "type": "number",
          "default": 2048,
          "description": "Maximum image size in KB after optimization (default: 2048)"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "jupyterLiveShare",
          "title": "Jupyter Live Share",
          "icon": "resources/icon.svg"
        }
      ],
      "panel": [
        {
          "id": "jupyterLiveShareChat",
          "title": "Viewer Chat",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "jupyterLiveShare": [
        {
          "id": "jupyterLiveShare.sessionView",
          "name": "Session",
          "type": "webview"
        }
      ],
      "jupyterLiveShareChat": [
        {
          "id": "jupyterLiveShare.viewerChatPanel",
          "name": "Chat",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "compile": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "lint": "eslint src --ext ts",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/sharp": "^0.31.1",
    "@types/vscode": "^1.82.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "copy-webpack-plugin": "^12.0.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.56.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sharp": "^0.34.5",
    "ws": "^8.16.0"
  }
}
